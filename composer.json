{
  "name": "yiimaker/yii2-helpers",
  "description": "Pack of useful helpers for Yii2 Framework",
  "keywords": ["yii2", "useful", "helpers", "util"],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "source": "https://github.com/yiimaker/yii2-helpers",
    "issues": "https://github.com/yiimaker/yii2-helpers/issues"
  },
  "authors": [
    {
      "name": "Vladimir Kuprienko",
      "email": "vldmr.kuprienko@gmail.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "yiisoft/yii2": "~2.0"
  },
  "require-dev": {
    "codeception/codeception": "~2.3",
    "codeception/verify": "*",
    "codeception/specify": "*"
  },
  "autoload": {
    "psr-4": {
      "ymaker\\helpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ymaker\\helpers\\tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "test": [
      "codecept build",
      "codecept run"
    ]
  }
}